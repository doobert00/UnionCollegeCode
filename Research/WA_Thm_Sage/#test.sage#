def setup():
	G = SymmetricGroup(3)
	A = G.algebra(QQ)
	print(A.basis().list())
	"""
	Returns a list of basis elements [b_1,...,b_n]
	
	where b = a_1g_1 + ... a_ng_n.
	note list(b) = [(g_1,a_1),...,(g_n,a_n))]
	so we can check if g_1\in list(G) have g_1 == list(b)[0][0]
	"""
	print(A.center_basis())
	
